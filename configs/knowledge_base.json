{
  "tunables": {
    "kernel.sched_min_granularity_ns": {
      "min": 100000,
      "max": 50000000,
      "default": 3000000,
      "effect": "latency"
    },
    "kernel.sched_latency_ns": {
      "min": 1000000,
      "max": 200000000,
      "default": 24000000,
      "effect": "latency"
    },
    "vm.dirty_background_ratio": {
      "min": 1,
      "max": 20,
      "default": 10,
      "effect": "io"
    },
    "vm.dirty_ratio": {
      "min": 5,
      "max": 40,
      "default": 20,
      "effect": "io"
    },
    "net.core.netdev_max_backlog": {
      "min": 1000,
      "max": 100000,
      "default": 10000,
      "effect": "net"
    },
    "kernel.sched_wakeup_granularity_ns": {
      "min": 1000000,
      "max": 100000000,
      "default": 15000000,
      "effect": "fairness"
    }
  },
  "dependencies": [
    {
      "if": "vm.dirty_ratio < vm.dirty_background_ratio",
      "action": "reject",
      "reason": "dirty_ratio must be >= dirty_background_ratio"
    }
  ],
  "blacklist": [
    "kernel.random.write_wakeup_threshold"
  ]
}